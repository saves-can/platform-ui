{
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "NITRO_PRESET=deno nuxi build",
    "preview": "cd .output/ && deno run -A server/index.ts",
    "deploy": "cd .output/ && deployctl deploy --project='clau-platform-ui server/index.ts && cd ..",
    "deploy:prod": "cd .output/ && deployctl deploy --project='clau-platform-ui' --prod server/index.ts && cd ..",
    "build:preview": "npm run build && npm run preview",
    "build:deploy": "npm run build && npm run deploy",
    "build:deploy:prod": "npm run build && npm run deploy:prod"
  },
